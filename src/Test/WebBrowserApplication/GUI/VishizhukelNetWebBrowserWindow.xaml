<Window x:Class="Aspenlaub.Net.GitHub.CSharp.VishizhukelNet.Test.WebBrowserApplication.GUI.VishizhukelNetWebBrowserWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        Title="VishizhukelNet WebBrowser Window" Height="295" MinHeight="295" Width="800" MinWidth="800" Icon="..\..\DemoApplication\GUI\demo.ico"
        Loaded="OnLoadedAsync" Closing="OnClosing" StateChanged="OnStateChanged">
    <DockPanel>
        <DockPanel DockPanel.Dock="Top">
            <Button Name="GoToUrl" DockPanel.Dock="Right" Padding="20,5,20,5">_Go</Button>
            <TextBox Name="Url" Padding="5"/>
        </DockPanel>
        <TabControl>
            <TabItem Header="Browser" Padding="60,5,60,5">
                <WebBrowser Name="WebBrowser" Navigating="OnWebBrowserNavigating" LoadCompleted="OnWebBrowserLoadCompleted" Navigated="OnWebBrowserNavigated" />
            </TabItem>
            <TabItem Header="Source" Padding="60,5,60,5">
                <TextBox Name="Source" Padding="5" FontFamily="Courier New"/>
            </TabItem>
        </TabControl>
    </DockPanel>
</Window>
